{
	"authors": [
		"klknn"
	],
	"buildTypes": {
		"release-native": {
			"buildOptions": [
				"releaseMode",
				"inline",
				"noBoundsCheck",
				"optimize"
			],
			"dflags-ldc": [
				"-mcpu=native"
			]
		},
		"unittest-native": {
			"buildOptions": [
				"inline",
				"optimize",
				"unittests"
			],
			"dflags-ldc": [
				"-mcpu=native"
			]
		},
		"unittest-opt": {
			"buildOptions": [
				"inline",
				"optimize",
				"unittests"
			]
		}
	},
	"configurations": [
		{
			"dependencies": {
				"dplug:lv2": "~>12.3.1"
			},
			"lflags-linux-ldc": [
				"--version-script=resource/module-lv2.ver"
			],
			"lflags-osx-arm64-ldc": [
				"-exported_symbols_list",
				"resource/module-lv2.lst"
			],
			"lflags-osx-x86_64-ldc": [
				"-exported_symbols_list",
				"resource/module-lv2.lst",
				"-dead_strip"
			],
			"name": "LV2",
			"targetType": "dynamicLibrary",
			"versions": [
				"LV2"
			]
		},
		{
			"dependencies": {
				"dplug:vst2": "~>12.3.1"
			},
			"lflags-linux-ldc": [
				"--version-script=resource/module-vst.ver"
			],
			"lflags-osx-ldc": [
				"-exported_symbols_list",
				"resource/module-vst.lst",
				"-dead_strip"
			],
			"name": "VST2",
			"targetType": "dynamicLibrary",
			"versions": [
				"VST2"
			]
		},
		{
			"dependencies": {
				"dplug:vst3": "~>12.3.1"
			},
			"lflags-linux-ldc": [
				"--version-script=resource/module-vst3.ver"
			],
			"lflags-osx-ldc": [
				"-exported_symbols_list",
				"resource/module-vst3.lst",
				"-dead_strip"
			],
			"name": "VST3",
			"targetType": "dynamicLibrary",
			"versions": [
				"VST3"
			]
		},
		{
			"dependencies": {
				"dplug:au": "~>12.3.1"
			},
			"lflags-osx-arm64-ldc": [
				"-exported_symbols_list",
				"resource/module-au.lst"
			],
			"lflags-osx-x86_64-ldc": [
				"-exported_symbols_list",
				"resource/module-au.lst",
				"-dead_strip"
			],
			"name": "AU",
			"targetType": "dynamicLibrary",
			"versions": [
				"AU"
			]
		}
	],
	"copyright": "Copyright Â© 2022, klknn",
	"dependencies": {
		"dplug:flat-widgets": "~>12.3.1",
		"dplug:pbr-widgets": "~>12.3.1",
		"mir-core": "~>1.1.90"
	},
	"description": "Electric piano plugin",
	"dflags-linux-dmd": [
		"-defaultlib=libphobos2.a"
	],
	"dflags-linux-ldc": [
		"-link-defaultlib-shared=false"
	],
	"dflags-linux-x86_64-ldc": [
		"-fvisibility=hidden"
	],
	"dflags-windows-ldc": [
		"-mscrtlib=libcmt",
		"-fvisibility=hidden",
		"-link-defaultlib-shared=false"
	],
	"license": "MIT, GPL v2 or any later version",
	"name": "epiano2",
	"stringImportPaths": [
		"."
	],
	"targetType": "dynamicLibrary"
}